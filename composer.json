{
    "name": "nerdofcode/laravel-sse",
    "description": "Server-Sent Events (SSE) library for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "sse", "server-sent-events", "streaming"],
    "authors": [
        {
            "name": "NerdOfCode",
            "email": "dev@nerdofcode.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^9.0|^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelSSE\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSSE\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html build/coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSSE\\SSEServiceProvider"
            ],
            "aliases": {
                "SSE": "LaravelSSE\\Facades\\SSE"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
